{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgresDb": "${{ PG_CONNECTION_STRING }}"
  },
  "OpenAi": {
    "ApiKey": "${{ OPEN_AI_API_KEY }}"
  },
  "Tooling": {
    "Swagger": {
      "Enabled": false
    }
  }
}
